/**
 * @param {object} workshop - A representation of the workshop
 * @param {string|number|boolean} gift - The gift to find
 * @returns {string[]} The path to the gift
 */
function findGiftPath(workshop, gift) {
  // FunciÃ³n auxiliar recursiva
  function search(obj, path) {
    // Recorrer todas las claves del objeto actual
    for (const key in obj) {
      const value = obj[key];
      
      // Si el valor es el regalo que buscamos, devolver el camino
      if (value === gift) {
        return [...path, key];
      }
      
      // Si el valor es un objeto, buscar recursivamente
      if (typeof value === 'object' && value !== null) {
        const result = search(value, [...path, key]);
        if (result.length > 0) {
          return result;
        }
      }
    }
    
    // No encontrado en este nivel
    return [];
  }
  
  return search(workshop, []);
}

// Pruebas
const workshop = {
  storage: {
    shelf: {
      box1: 'train',
      box2: 'switch'
    },
    box: 'car'
  },
  gift: 'doll'
};

console.log(findGiftPath(workshop, 'train'));  // ['storage', 'shelf', 'box1']
console.log(findGiftPath(workshop, 'switch')); // ['storage', 'shelf', 'box2']
console.log(findGiftPath(workshop, 'car'));    // ['storage', 'box']
console.log(findGiftPath(workshop, 'doll'));   // ['gift']
console.log(findGiftPath(workshop, 'plane'));  // []